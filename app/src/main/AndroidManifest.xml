<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.beautycenter.android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:usesCleartextTraffic="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.beautycenter.android.presentation.ui.reserve.ReserveActivity">
        </activity>
        <activity android:name="com.beautycenter.android.presentation.ui.test.TestLoginActivity">
        </activity>
        <activity
                android:name="com.beautycenter.android.presentation.ui.servicedetail.ServiceDetailActivity"
                android:parentActivityName="com.beautycenter.android.presentation.ui.mainnavigation.NavigationActivity">
        </activity>
        <activity
                android:name="com.beautycenter.android.presentation.ui.mainnavigation.NavigationActivity"
                android:label="@string/title_activity_navigation"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="com.beautycenter.android.presentation.ui.registry.RegistryActivity"
                android:parentActivityName="com.beautycenter.android.presentation.ui.login.MainActivity">
        </activity>
        <activity android:name="com.beautycenter.android.presentation.ui.login.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>